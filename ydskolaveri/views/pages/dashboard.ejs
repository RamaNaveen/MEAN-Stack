<%-contentFor('hero')%>


<div id ="des">

<h3><center><b>Approved Projects</b></center></h3>
<br>
<br>
<div id=inds>
<%if (dashboardprojectsArray.length == 0){ %>
<div id="success">

  <h1>There are no projects to show</h1>

  </div>
<%}%> 
<% for(var i = 0;i<dashboardprojectsArray.length;i++){%>

 <div id="indi">
 <div id="imgin">
 <img src="images/<%-dashboardprojectsArray[i].image%>" id="poi">
 </div>
 <div id="in">
<div id="imgio">
<center><a href="/description?id=<%-dashboardprojectsArray[i]._id%>"><b><%-dashboardprojectsArray[i].title%></a></center><br>
<%-dashboardprojectsArray[i].description%>
</div>
<div id="imgip">
<div id="pgbar">
<div id="pgbarfill" style="width:<%= (dashboardprojectsArray[i].fundscollected/dashboardprojectsArray[i].funds)*100 %>%" ><br><br>
</div></div></div>
<div id="imgik">
<div id="l1">Funded<br><%= ((dashboardprojectsArray[i].fundscollected/dashboardprojectsArray[i].funds)*100).toFixed(2) %>%</div>
<div id="l2">Target<br><%-dashboardprojectsArray[i].funds%> </div>
<div id="l3">Deadline<br><%-dashboardprojectsArray[i].deadline%></div>
   

 
 
</div>
</div>
</div>

<%}%> 
</div>
<hr>
<h3><center><b>Pending Projects</b></center></h3><br>
<br>
